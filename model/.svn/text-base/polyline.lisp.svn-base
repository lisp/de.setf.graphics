;;;; --------------------------------------------------------------------------
;;;; i/o functions



(defObFun (print-object polygon)
  (stream)
  (write-string "#g(polygon " stream)
  (write (polygon-points self) :stream stream)
  (write-char #\space stream)
  (write (polygon-properties self) :stream stream)
  (write-char #\) stream))


